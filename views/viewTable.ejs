<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>TABLE JSON</title>
</head>
<body>

    <table>
        <thead>
            <tr>
                <th>Columna</th>
                <th>DB</th>
            </tr>
        </thead>
        <tbody>
            <% json.fields.forEach(element =>{ %>
            <tr>
                <td><%= element.column_name %></td>
                <td><%= element.table_schema %></td>
            </tr>
            <%}) %>

        </tbody>
    </table>
    
</body>
</html>